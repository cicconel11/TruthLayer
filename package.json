{
    "name": "truthlayer-mvp",
    "version": "1.0.0",
    "description": "Infrastructure system for search and AI transparency",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "dev": "tsx watch src/index.ts",
        "start": "node dist/index.js",
        "test": "vitest --run",
        "test:watch": "vitest",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "typecheck": "tsc --noEmit",
        "migrate": "tsx src/database/migrate.ts",
        "seed": "tsx src/database/seed.ts",
        "dashboard:api": "tsx scripts/start-dashboard.ts",
        "dashboard:dev": "cd dashboard && npm run dev",
        "dashboard:build": "cd dashboard && npm run build",
        "dashboard:start": "cd dashboard && npm run start",
        "integrity:validate": "tsx src/utils/data-integrity-cli.ts validate",
        "integrity:completeness": "tsx src/utils/data-integrity-cli.ts completeness",
        "integrity:coverage": "tsx src/utils/data-integrity-cli.ts coverage",
        "integrity:hashes": "tsx src/utils/data-integrity-cli.ts generate-hashes",
        "integrity:duplicates": "tsx src/utils/data-integrity-cli.ts duplicates",
        "debug": "tsx src/utils/debug-cli.ts",
        "debug:audit": "tsx src/utils/debug-cli.ts audit",
        "debug:monitor": "tsx src/utils/debug-cli.ts monitor",
        "debug:analyze": "tsx src/utils/debug-cli.ts analyze"
    },
    "keywords": [
        "search-transparency",
        "algorithmic-bias",
        "data-collection",
        "web-scraping",
        "llm-annotation"
    ],
    "author": "TruthLayer Team",
    "license": "MIT",
    "dependencies": {
        "2captcha-ts": "^2.4.1",
        "anticaptcha": "^2.2.0",
        "axios": "^1.6.2",
        "capsolver-npm": "^2.1.10",
        "cheerio": "^1.0.0-rc.12",
        "commander": "^11.1.0",
        "cors": "^2.8.5",
        "date-fns": "^2.30.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "next": "^14.0.3",
        "node-cron": "^3.0.3",
        "openai": "^4.20.1",
        "pg": "^8.11.3",
        "puppeteer": "^21.5.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "uuid": "^9.0.1",
        "winston": "^3.11.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.35",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.9.0",
        "@types/node-cron": "^3.0.11",
        "@types/pg": "^8.10.7",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.54.0",
        "eslint-config-next": "^14.0.3",
        "postcss": "^8.4.31",
        "prettier": "^3.1.0",
        "tailwindcss": "^3.3.5",
        "tsx": "^4.6.0",
        "typescript": "^5.2.2",
        "vitest": "^0.34.6"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    }
}