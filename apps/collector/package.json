{
  "name": "@truthlayer/collector",
  "version": "0.1.0",
  "private": true,
  "description": "TruthLayer multi-engine collector service",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "dev": "ts-node-esm --loader ts-node/esm src/index.ts",
    "lint": "eslint src --ext .ts",
    "start": "node dist/index.js",
    "test": "vitest run"
  },
  "dependencies": {
    "@truthlayer/config": "link:../../packages/config",
    "@truthlayer/schema": "link:../../packages/schema",
    "@types/cheerio": "^1.0.0",
    "axios": "1.7.2",
    "cheerio": "^1.1.2",
    "p-queue": "8.0.1",
    "p-retry": "5.1.2",
    "p-timeout": "6.0.0",
    "puppeteer": "22.8.0",
    "winston": "3.13.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "ts-node": "10.9.2",
    "tsup": "8.1.0",
    "typescript": "5.4.5",
    "vitest": "1.6.0"
  }
}
